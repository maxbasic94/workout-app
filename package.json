{
  "name": "workout",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.37",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "eslint-config-react-app": "^6.0.0",
    "react": "^17.0.2",
    "react-countdown-circle-timer": "^2.5.4",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-router-dom": "^6.0.2",
    "react-scripts": "4.0.3",
    "styled-components": "^5.3.3",
    "typescript": "^4.5.2",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint --ext .ts, .tsx src --fix",
    "test": "jest",
    "eject": "react-scripts eject",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"    
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|styl|less|sass|scss|png|jpg|jpeg|ttf|woff|woff2)$": "identity-obj-proxy"
    },
    "compilerOptions": {
      "module": "commonjs",
      "target": "esnext",
      "jsx": "react",
      "sourceMap": false,
      "experimentalDecorators": true,
      "noImplicitUseStrict": true,
      "removeComments": true,
      "moduleResolution": "node",
      "lib": [
        "es2017",
        "dom"
      ],
      "typeRoots": [
        "node_modules/@types",
        "src/@types"
      ]
    },
    "exclude": [
      "node_modules",
      "out",
      ".next"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 100,
    "endOfLine": "auto"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier"
    ],
    "rules": {
      "jsx-quotes": [
        1,
        "prefer-double"
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "main": "index.js",
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/plugin-transform-modules-commonjs": "^7.16.5",
    "@babel/plugin-transform-react-jsx": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.5",
    "@testing-library/react": "^11.2.7",
    "@types/enzyme": "^3.10.10",
    "@types/pretty": "^2.0.0",
    "@types/react-test-renderer": "^17.0.1",
    "@types/styled-components": "^5.1.18",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "babel-jest": "^26.6.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-17-updated": "^1.0.2",
    "enzyme-to-json": "^3.6.2",
    "eslint": "7.32.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "husky": "^7.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "26.6.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^12.1.5",
    "node-fetch": "^3.1.0",
    "prettier": "2.5.0",
    "pretty": "^2.0.0",
    "react-switch": "^6.0.0",
    "regenerator-runtime": "^0.13.9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxbasic94/workout-app.git"
  },
  "author": "Maksim Ashmiana",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/maxbasic94/workout-app/issues"
  },
  "cspell": {
    "version": "0.2",
    "ignorePaths": [],
    "dictionaryDefinitions": [],
    "dictionaries": [],
    "words": [],
    "ignoreWords": [
      "Maksim",
      "Ashmiana",
      "browserslist",
      "styl",
      "precommit"
    ],
    "import": []
  }
}
